package GameTest;
import java.util.Random;

public class simplemath {
	
	Random rand = new Random();
	
	int gen1, gen2, solve;

	public simplemath() {
		// TODO Auto-generated constructor stub
		gen1 = rand.nextInt(10);
		gen2 = rand.nextInt(10);
	}
	
	public simplemath(int ex1, int ex2) {
		gen1 = ex1;
		gen2 = ex2;
	}
	
	public int addition() {
		solve = gen1 + gen2;
		return solve;
	}
	
	public int subtraction() {
		solve = gen1-gen2;
		return solve;
	}
	
	public int getGen1() {
		return gen1;
	}
	
	public int getGen2() {
		return gen2;
	}

}
