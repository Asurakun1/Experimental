package GameTest;
import java.util.Random;

public class simplemath {
	
	Random rand = new Random();
	
	int gen1, gen2, solve;

	public simplemath() {
		// TODO Auto-generated constructor stub
		gen1 = rand.nextInt(10);
		gen2 = rand.nextInt(10);
	}
	
	public simplemath(int ex1, int ex2) {
		gen1 = ex1;
		gen2 = ex2;
	}
	
	
	

}
