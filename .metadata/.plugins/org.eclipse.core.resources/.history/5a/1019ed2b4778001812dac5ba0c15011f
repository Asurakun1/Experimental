	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Stats newChar = new Stats();
		
		Random rando = new Random();
		
		newChar.getStats();
//		newChar.addExp(1101);
		for(int i = 0; i < 45; i++) {
			newChar.addExp(rando.nextInt(100)*2.543112);
		}
	}

}
